<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE sqlMapConfig
  PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings cacheModelsEnabled="true" enhancementEnabled="false"
		lazyLoadingEnabled="false" maxRequests="3000" maxSessions="3000"
		maxTransactions="3000" useStatementNamespaces="true" />
	<sqlMap resource="sqlmap/user.xml" />
	<sqlMap resource="sqlmap/menu.xml" />
	<sqlMap resource="sqlmap/position.xml" />
	<sqlMap resource="sqlmap/organize.xml" />
	<sqlMap resource="sqlmap/logs.xml" />
	<sqlMap resource="sqlmap/roleright.xml" />
	<sqlMap resource="sqlmap/role.xml" />
	<sqlMap resource="sqlmap/oftenoperate.xml" />
	<sqlMap resource="sqlmap/parach.xml" />
	<sqlMap resource="sqlmap/paravalue.xml" />
</sqlMapConfig>























































